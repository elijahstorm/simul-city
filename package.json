{
	"name": "simul-city",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"release": "git stash && pnpm releaseBuild ; git stash pop || echo 'Finished with a clean working tree'",
		"releaseBuild": "git checkout release && git merge main --no-edit && pnpm build && git add docs/ && git commit -m \"Released: `date +'%Y-%m-%d %H:%M:%S'`\" && git push && git checkout main",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.11.1",
		"@iconify/svelte": "^3.1.0",
		"@sveltejs/adapter-static": "2.0.1",
		"@sveltejs/kit": "^1.9.1",
		"@types/gl-vec2": "^1.3.2",
		"@types/three": "^0.149.0",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"rxjs": "^7.8.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^5.0.9",
		"@threlte/extras": "^4.9.3",
		"@threlte/rapier": "^0.5.0",
		"fp-ts": "^2.13.1",
		"gl-vec2": "^1.3.0",
		"three": "^0.150.1"
	}
}
